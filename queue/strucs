; The order of the strucs' fields is required for the cmpxchg16b techniques.

; TODO: Instances of these strucs must be aligned at 8-byte, so that loads and
; stores are atomic.

struc queue
  .head: resq 1
  .tail: resq 1
endstruc

struc queue_node
  .head_of: resq 1
  .locked:  resq 1
  .tail_of: resq 1
  .next:    resq 1
endstruc
